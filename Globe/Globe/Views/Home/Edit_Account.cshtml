@model Edit_Account
<div class="main_contant">
    <div class="title">Edit Account</div>
    <form class="Form" asp-action="Edit_Account">
        <div class="AE_F_Text">
            <label>First Name</label>
            <input type="text" asp-for="fname" placeholder="Change First Name">
        </div>

        <div class="AE_F_Text">
            <label>Last Name</label>
            <input type="text" asp-for="lname" placeholder="Change Last Name">
        </div>

        <div class="AE_F_Text">
            <input type="button" name="title" value="submit">
        </div>
    </form>

</div>